{
	"C_Cpp.loggingLevel": "None",
	"debug.toolBarLocation": "docked",
	"editor.tabSize": 4,
	"editor.insertSpaces": false,
	"editor.formatOnSave": true,
	"files.encoding": "utf8",
	"files.trimTrailingWhitespace": true,
	"files.eol": "\n",
	"files.exclude": {
		"vendor": true,
		"chalet_external": true,
		"dist": true,
		"**/*.7z": true,
		"**/*.rar": true,
		"**/Thumbs.db": true
	},
	"search.exclude": {
		"chalet_external": true,
		"build": true,
		"platform": true,
	},
	"files.associations": {
		"*.json": "jsonc",
		"*.stats": "cpp",
		"*.desktop": "properties",
		".clang-format": "yaml"
	},
	"[applescript]": {
		"files.encoding": "utf8"
	}
}